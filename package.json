{
  "name": "vuloon",
  "version": "0.0.0",
  "description": "Vuloon",
  "private": true,
  "workspaces": [
    "packages/@vuloon/*"
  ],
  "scripts": {
    "build": "turbo run build"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
